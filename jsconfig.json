{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "modules/*": [
        "src/modules/*"
      ],
      "lib/*": [
        "src/lib/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}